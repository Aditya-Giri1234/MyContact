<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/contactListFragment2">
    <fragment
        android:id="@+id/contactListFragment2"
        android:name="com.aditya.mycontact.ui_layer.ui.ContactListFragment"
        android:label="fragment_contact_list"
        tools:layout="@layout/fragment_contact_list" >
        <action
            android:id="@+id/action_contactListFragment2_to_addContactFragment2"
            app:destination="@id/addContactFragment2" />
    </fragment>
    <fragment
        android:id="@+id/addContactFragment2"
        android:name="com.aditya.mycontact.ui_layer.ui.AddContactFragment"
        android:label="fragment_add_contact"
        tools:layout="@layout/fragment_add_contact" >
        <argument
            android:name="id"
            app:argType="string"
            app:nullable="true"/>
    </fragment>
</navigation>